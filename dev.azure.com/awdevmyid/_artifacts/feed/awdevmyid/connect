az devops login --organization https://dev.azure.com/awdevmyid/
az artifacts universal download --organization https://dev.azure.com/awdevmyid/ --feed awdevmyid --name my-first-package --version 0.0.1 --path .
az artifacts universal publish --organization https://dev.azure.com/awdevmyid/ --feed awdevmyid --name my-first-package --version 0.0.1 --description "Welcome to Universal Packages" --path .
